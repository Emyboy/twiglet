  },
  "homepage": "https://github.com/emyboy/twiglet#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.6.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
